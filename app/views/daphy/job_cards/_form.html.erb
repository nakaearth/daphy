<div class='job_form' id='job_form'>
  <%= form_for @job_card, url: { action: 'create', controller: 'daphy/job_cards' }, :html => {:multipart => true} do |f| %>
    <%= f.label :title %>:<%= f.text_field :title %><br/>
    <%= f.label :description %>:<%= f.text_area :description, :size => "80x20" %><br/>
    <%= f.submit %>
  <% end %> 
</div>
